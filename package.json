{
  "name": "@geek/eslint-config",
  "version": "1.0.1",
  "description": "ESLint base configuration for Node.js development",
  "main": ".eslintrc.js",
  "repository": {
    "type": "git",
    "url": "github:brentonhouse/geek-eslint-config.git"
  },
  "scripts": {
    "list": "npm pack && tar -xvzf *.tgz && rm -rf package *.tgz",
    "test": "npm install --ignore-scripts && npm run list",
    "prepublishOnly": "git pull && bump release"
  },
  "keywords": [
    "eslint",
    "nodejs",
    "brenton-house"
  ],
  "author": {
    "name": "Brenton House",
    "email": "brenton.house@gmail.com",
    "url": "https://brenton.house"
  },
  "license": "MIT",
  "dependencies": {
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^34.7.0",
    "eslint-plugin-jsonc": "^0.8.2",
    "eslint-plugin-promise": "^4.2.1"
  },
  "peerDependencies": {
    "eslint": "^7.18.0"
  },
  "devDependencies": {
    "eslint": "^7.18.0"
  }
}
