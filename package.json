{
  "name": "@geek/eslint-config",
  "version": "0.0.10",
  "description": "ESLint base configuration for Node.js development",
  "main": ".eslintrc.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brentonhouse/geek-eslint-config.git"
  },
  "scripts": {
    "list-files": "npm pack && tar -xvzf *.tgz && rm -rf package *.tgz",
    "test": "npm install --ignore-scripts && npm run list-files",
    "prepublishOnly": "git pull && bump"
  },
  "keywords": [
    "eslint",
    "nodejs",
    "brenton-house"
  ],
  "author": {
    "name": "Brenton House",
    "email": "brenton.house@gmail.com",
    "url": "https://brenton.house"
  },
  "license": "MIT",
  "dependencies": {
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jsdoc": "^27.1.1",
    "eslint-plugin-promise": "^4.2.1"
  }
}
